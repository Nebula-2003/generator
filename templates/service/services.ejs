import <%= serviceName %>Model from './<%= serviceName %>.model.js';

/**
 * add
 */
export const add = async (reqBody) => {
    return await <%= serviceName %>Model(reqBody).save();
}

/**
 * Get
 */
export const get = async (id) => {
    return await <%= serviceName %>Model.findOne({ _id: id }).sort({ created_at: -1 }).lean();
}

/**
 * List
 */
export const list = async (query) => {
    return await <%= serviceName %>Model.find(query).lean();
}

/**
 * update
 */
export const update = async (id, reqBody) => {
    return await <%= serviceName %>Model.findByIdAndUpdate({ _id: id }, { $set: reqBody }, { new: true }).lean();
}

/**
 * Delete
 */
export const delete = async (id) => {
    return await <%= serviceName %>Model.findByIdAndDelete({ _id: id }).lean();
}
